{
    "name": "Mockoon TUS Server",
    "port": 3000,
    "routes": [
        {
            "method": "OPTIONS",
            "path": "/files",
            "response": {
                "status": 204,
                "headers": {
                    "Access-Control-Allow-Origin": "*",
                    "Access-Control-Allow-Methods": "OPTIONS, POST, HEAD, PATCH",
                    "Access-Control-Allow-Headers": "Tus-Resumable, Upload-Length, Upload-Offset, Upload-Metadata",
                    "Access-Control-Expose-Headers": "Tus-Resumable, Upload-Length, Upload-Offset, Upload-Metadata",
                    "Tus-Resumable": "1.0.0",
                    "Content-Length": "0"
                }
            }
        },
        {
            "method": "POST",
            "path": "/files",
            "response": {
                "status": 201,
                "headers": {
                    "Access-Control-Allow-Origin": "*",
                    "Access-Control-Allow-Methods": "OPTIONS, POST, HEAD, PATCH",
                    "Access-Control-Allow-Headers": "Tus-Resumable, Upload-Length, Upload-Offset, Upload-Metadata",
                    "Access-Control-Expose-Headers": "Tus-Resumable, Upload-Length, Upload-Offset, Upload-Metadata",
                    "Tus-Resumable": "1.0.0",
                    "Location": "/files/123456",
                    "Content-Length": "0"
                }
            }
        },
        {
            "method": "HEAD",
            "path": "/files/:fileId",
            "response": {
                "status": 200,
                "headers": {
                    "Access-Control-Allow-Origin": "*",
                    "Access-Control-Allow-Methods": "OPTIONS, POST, HEAD, PATCH",
                    "Access-Control-Allow-Headers": "Tus-Resumable, Upload-Length, Upload-Offset, Upload-Metadata",
                    "Access-Control-Expose-Headers": "Tus-Resumable, Upload-Length, Upload-Offset, Upload-Metadata",
                    "Tus-Resumable": "1.0.0",
                    "Upload-Length": "1024",
                    "Upload-Offset": "512",
                    "Content-Length": "0"
                }
            }
        },
        {
            "method": "PATCH",
            "path": "/files/:fileId",
            "response": {
                "status": 204,
                "headers": {
                    "Access-Control-Allow-Origin": "*",
                    "Access-Control-Allow-Methods": "OPTIONS, POST, HEAD, PATCH",
                    "Access-Control-Allow-Headers": "Tus-Resumable, Upload-Length, Upload-Offset, Upload-Metadata",
                    "Access-Control-Expose-Headers": "Tus-Resumable, Upload-Length, Upload-Offset, Upload-Metadata",
                    "Tus-Resumable": "1.0.0",
                    "Upload-Offset": "1024",
                    "Content-Length": "0"
                }
            }
        }
    ]
}